{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "ONET_USERNAME": "ONET_USERNAME",
    "ONET_PASSWORD": "ONET_PASSWORD"
  }
}
